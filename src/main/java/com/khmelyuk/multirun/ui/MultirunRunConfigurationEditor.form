    <constraints>
      <xy x="20" y="20" width="684" height="405"/>
    </constraints>
    <properties/>
    <border type="line">
      <color color="-10066330"/>
    </border>
    <children>
      <grid id="41447" binding="myMainPanel" layout-manager="GridLayoutManager" row-count="6" column-count="8" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints>
